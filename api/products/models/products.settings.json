{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "price": {
      "type": "biginteger"
    },
    "stock": {
      "type": "integer"
    },
    "category": {
      "model": "categories"
    }
  }
}
